{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "commonjs": true
    },
    "plugins": [
        "promise",
        "angular"
    ],
    "globals": {
        "angular": 1
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"]
    }
}